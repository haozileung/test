  "main": "app.js",
  "scripts": {
    "start": "spm build -c && set NODE_ENV=production && node ./bin/www"
  },
  "spm": {
    "output": [
      "./admin/*.js"
    ],
    "build": {
      "dest": "./public/assets/dist",
      "publicPath": "http://localhost:8000/",
      "extractCSS": true,
      "loader": {
        ".js": "+./admin/lib/moduleGen"
      }
    }
  },
  "dependencies": {
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "jade": "~1.11.0",
    "moment": "^2.10.6",
    "morgan": "~1.6.1",
    "serve-favicon": "~2.3.0"
  }
}
